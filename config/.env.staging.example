# Staging Environment Configuration
# Copy this file to .env.staging and update with your values

# Environment
ENV=staging

# Telegram Bot Configuration
BOT_TOKEN=your_staging_bot_token_here
ADMIN_TELEGRAM_ID=123456789

# Database (PostgreSQL or similar to production)
DATABASE_URL=postgresql://user:password@localhost:5432/bankbot_staging

# Parsing (enabled in staging)
PARSING_ENABLED=true

# Logging (verbose for staging)
LOG_LEVEL=DEBUG
LOG_FILE=logs/bot_staging.log

# Background Tasks
TASK_CHECK_INTERVAL=300
